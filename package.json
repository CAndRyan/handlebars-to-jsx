{
  "name": "handlebars-to-jsx",
  "version": "0.0.9",
  "author": "Danakt Frost <danakt@protonmail.com>",
  "license": "MIT",
  "description": "Converts Handlebars template to React component",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "handlebars",
    "jsx",
    "react",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danakt/handlebars-to-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/danakt/handlebars-to-jsx/issues"
  },
  "homepage": "https://github.com/danakt/handlebars-to-jsx",
  "typings": "index.d.ts",
  "devDependencies": {
    "@types/babel__generator": "^7.0.0",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/object-hash": "^1.2.0",
    "@types/prettier": "^1.13.2",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-arca": "^0.6.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.8.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^20.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.1.3",
    "@babel/parser": "^7.1.3",
    "@babel/types": "^7.1.3",
    "@glimmer/syntax": "^0.36.5",
    "is-self-closing": "^1.0.1",
    "prettier": "^1.14.3",
    "react-attr-converter": "^0.3.1",
    "reserved-words": "^0.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**",
      "ui/**"
    ]
  }
}
